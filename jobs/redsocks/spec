---
name: redsocks
packages: []
templates:
  config/bpm.yml: config/bpm.yml
  config/redsocks.conf.erb: config/redsocks.conf
  post-deploy.erb: bin/post-deploy
  post-start.erb: bin/post-start
  pre-start.erb: bin/pre-start
  post-stop.erb: bin/post-stop




consumes: []
provides:
- name: redsocks
  type: redsocks
properties:
  local-ip:
    description: local listening ip for redsocks, default to 127.0.0.1.
  remote-proxy.ip:
    description: relay traffic to this host
  remote-proxy.port:
    description: relay traffic to this host
    default: 3128
  port:
    description: Bind redsocks process to this port
    default: 12346
    
